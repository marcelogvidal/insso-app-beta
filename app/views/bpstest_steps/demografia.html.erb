        <div class="form-group">
          <%= b.label :Tiempo_que_lleva_trabajando_en_este_puesto  %>
        </div>
        <div class="form-group">
          <%= number_field_tag :dgp8 %>
        </div>
        <div class="form-group">
          <%= b.label :Cuanto_años_de_estudio_tiene_Ud_a_contar_de_primero_básico  %>
