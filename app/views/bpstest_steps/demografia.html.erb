    <div> error_messages </div>
    <% b.object.errors.full_messages.each do |error| %>
    <li><%= message %></li>
    <% end %>
    <% end %>



    <div class="thumbnail">
      <div class="well">
        <div class="form-group">
          <%= b.label :Género %>
        </div>
        <div class="form-group">
          <%= b.text_field :dgp1 %>
        </div>
        <div class="form-group">
          <%= b.label :Fecha_de_Nacimiento  %>
        </div>
        <div class="form-group">
          <%= date_field_tag :dgp2 %>
        </div>
        <div class="form-group">
          <%= label_tag (:Estado_civil)  %>
        </div>
        <div class="form-group">
          <%= label_tag(:Estadocivil, "Casado") %>
          <%= radio_button_tag(:dgp3, "Casado") %>
        </div>
        <div class="form-group">
          <%= label_tag(:Estadocivil, "Soltero") %>
          <%= radio_button_tag(:dgp3, "Soltero") %>
        </div>
        <div class="form-group">
          <%= label_tag(:Estadocivil, "Viudo") %>
          <%= radio_button_tag(:dgp3, "Viudo") %>
