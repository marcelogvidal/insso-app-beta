<h1>Listing bpstests</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bpstests.each do |bpstest| %>
      <tr>
        <td><%= link_to 'Show', bpstest %></td>
        <td><%= link_to 'Edit', edit_bpstest_path(bpstest) %></td>
        <td><%= link_to 'Destroy', bpstest, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bpstest', new_bpstest_path %>
