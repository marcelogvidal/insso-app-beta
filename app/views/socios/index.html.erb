
<%= render partial: "elements/sidebar" %>

<div class="container-fluid">

  <div class="panel-heading">
    <h3 class="panel-title">DATOS SOCIO DEMOGRÁFICO</h3>
  </div>
  <div class="panel-body">
<table>
  <thead>
    <tr>
      <th>Sexo</th>
      <th>Worker</th>
      <th>Fecha nacimiento</th>
      <th>Edad</th>
      <th>Estado civil</th>
      <th>Hijos</th>
      <th>Cuantos viven con Ud.</th>
      <th>Tipo de contrato</th>
      <th>Tiempo de ocupación</th>
      <th>Tiempo en la organización</th>
      <th>Tiempo en el puesto</th>
      <th>Años de estudio</th>
      <th>Cargo actual</th>
      <th>Unidad actual</th>
      <th>Region</th>
      <th>Ciudad</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @socios.each do |socio| %>
      <tr>
        <td><%= socio.sexo %></td>
        <td><%= socio.worker %></td>
        <td><%= socio.fecha %></td>
        <td><%= socio.edad %></td>
        <td><%= socio.civil2 %></td>
        <td><%= socio.hijo %></td>
        <td><%= socio.hijo2 %></td>
        <td><%= socio.contrato %></td>
        <td><%= socio.ocupacion %></td>
        <td><%= socio.temporga %></td>
        <td><%= socio.temptra %></td>
        <td><%= socio.anoestu %></td>
        <td><%= socio.cargo %></td>
        <td><%= socio.unidad %></td>
        <td><%= socio.region %></td>
        <td><%= socio.ciudad %></td>
        <td><%= link_to 'Show', socio %></td>
        <td><%= link_to 'Edit', edit_socio_path(socio) %></td>
        <td><%= link_to 'Destroy', socio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

</div>
</div>

<br>

<%= link_to 'New Socio', new_socio_path %>
