<%= simple_form_for(@worker) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :lastname %>
    <%= f.association :company %>
    <%= f.association :division %>
    <%= f.input :rut %>
    <%= f.association :bpstest %>
    <%= f.input :file %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
