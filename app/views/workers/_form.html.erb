<div class="modal-dialog">
  <div class="modal-content">
    <div class="row">
      <div class="col-md-6 col-md-offset-3">

  <h2 align="center"><%=t ".title", default: "Ingrese nuevo trabajador" %></h2>
<%= simple_form_for(@worker, html: { multipart: true }) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :lastname %>
    <%= f.association :company %>
    <%= f.association :division %>
    <%= f.input :rut %>
    <%= f.association :bpstest %>
    <%= f.input :file %>
    <%= f.input :picture %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>

</div>
</div>
</div>
</div>
