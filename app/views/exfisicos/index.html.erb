<h1>Listing exfisicos</h1>

<table>
  <thead>
    <tr>
      <th>Peso</th>
      <th>Worker</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @exfisicos.each do |exfisico| %>
      <tr>
        <td><%= exfisico.peso %></td>
        <td><%= exfisico.worker %></td>
        <td><%= link_to 'Show', exfisico %></td>
        <td><%= link_to 'Edit', edit_exfisico_path(exfisico) %></td>
        <td><%= link_to 'Destroy', exfisico, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Exfisico', new_exfisico_path %>
